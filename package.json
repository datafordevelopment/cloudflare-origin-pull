{
  "name": "cloudflare-origin-pull",
  "version": "0.1.0",
  "description": "Verify that TLS peer certificates came from CloudFlare",
  "main": "index.js",
  "files": [
    "index.js",
    "index.js.map",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf index.js index.js.map lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir ./ --source-maps",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm run build",
    "coverage": "nyc npm test",
    "test": "ava",
    "posttest": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/novemberborn/cloudflare-origin-pulls.git"
  },
  "keywords": [
    "cloudflare",
    "tls",
    "ssl",
    "https"
  ],
  "author": "Mark Wubben (https://novemberborn.net/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/novemberborn/cloudflare-origin-pulls/issues"
  },
  "homepage": "https://github.com/novemberborn/cloudflare-origin-pulls#readme",
  "dependencies": {
    "asn1.js": "^4.5.0"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "lolex": "^1.4.0",
    "nyc": "^5.6.0",
    "rimraf": "^2.5.2",
    "standard": "^6.0.7"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
